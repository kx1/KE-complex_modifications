{
    "title": "Personal rules (@kx1) experimental",
    "rules": [
        {
            "description": "Change spacebar to right_command if pressed with other keys (Post spacebar when pressed alone)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("spacebar", [], ["any"]) %>,
                    "to": <%= to([["right_shift"]]) %>,
                    "to_if_alone": <%= to([["spacebar"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+h to g",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("h", ["right_shift"], ["any"]) %>,
