        },
        {
            "description": "Change right_shift+i to e",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("i", ["right_shift"], ["any"]) %>,
                    "to": <%= to([["e"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+o to w",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("o", ["right_shift"], ["any"]) %>,
                    "to": <%= to([["w"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+p to q",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("p", ["right_shift"], ["any"]) %>,
                    "to": <%= to([["q"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+p to q",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("p", ["right_shift"], ["any"]) %>,
                    "to": <%= to([["q"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+n to b",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("n", ["right_shift"], ["any"]) %>,
                    "to": <%= to([["b"]]) %>
                }
            ]
        },
        {
            "description": "Change right_shift+m to v",
            "manipulators": [
                {
